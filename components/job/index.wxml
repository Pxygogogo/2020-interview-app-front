<view class="container" >
  <view wx:for="{{model}}" wx:key="name" class="avatar-box {{showId == item.id ? 'active' : ''}}" bindtap="handleDiscussInfo" data-id="{{item.id}}">
    <view class="img-box">
      <image src="{{item.iconUrl}}" class="img"></image>
    </view>
    <text>{{item.name}}</text>
  </view>
  
</view>